@model List<SPORSALONUYONETIM.Models.Trainer>

@{
    ViewData["Title"] = "Antrenörler";
}

<h2>Antrenörler</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Ad Soyad</th>
            <th>Spor Türü</th>
            <th>Çalışma Başlangıç</th>
            <th>Çalışma Bitiş</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in Model)
        {
            <tr>
                <td>@t.FullName</td>
                <td>@t.SportType</td>
                <td>@t.WorkStart</td>
                <td>@t.WorkEnd</td>
            </tr>
        }
    </tbody>
</table>
